{
  "manifest_version": 2,
  "name": "AnonIn",
  "description": "ðŸ‘¤ Automatically change LinkedIn visibility to private before visiting a person's profile",
  "version": "0.0.1",
  "icons": {
    "64": "icons/icon.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*.linkedin.com/*"],
      "js": ["linkedin_observer.js"],
      "run_at": "document_start"
    }
  ],
  "background": {
    "scripts": ["browser-polyfill.min.js", "background_script.js"]
  },
  "page_action": {
    "default_icon": {
      "64": "icons/icon.png"
    },
    "default_popup": "pageAction/index.html",
    "default_title": "AnonIn"
  },
  "permissions": [
    "activeTab",
    "webRequest",
    "webRequestBlocking",
    "https://www.linkedin.com/psettings/profile-visibility",
    "*://*.linkedin.com/*"
  ]
}
