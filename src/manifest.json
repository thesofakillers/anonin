{
  "manifest_version": 2,
  "name": "AnonIn",
  "description": "ðŸ‘¤ Automatically change LinkedIn visibility to private before visiting a person's profile",
  "version": "0.0.1",
  "icons": {
    "64": "icons/icon.png"
  },
  "background": {
    "scripts": ["background_script.js", "browser-polyfill.min.js"]
  },
  "content_scripts": [
    {
      "matches": ["*://*.mozilla.org/*"],
      "js": ["content_script.js", "browser-polyfill.min.js"]
    }
  ],
  "page_action": {
    "default_icon": {
      "64": "icons/icon.png"
    },
    "default_popup": "pageAction/index.html",
    "default_title": "AnonIn"
  }
}

